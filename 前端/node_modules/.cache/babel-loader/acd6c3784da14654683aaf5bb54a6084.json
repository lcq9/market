         收货地址\r\n        </div>\r\n        <div style=\"padding: 10px 0; color: #555\" v-for=\"item in addressData\" :key=\"item.id\">\r\n          <el-radio v-model=\"chooseAddress\" :label=\"item\" border>{{ item.linkUser }}</el-radio>\r\n          <span>{{ item.linkUser }}</span>\r\n          <span style=\"margin-left: 20px\">{{ item.linkPhone }}</span>\r\n          <span style=\"margin-left: 20px\">{{ item.linkAddress }}</span>\r\n        </div>\r\n      </div>\r\n\r\n\r\n      <!--        商品确认-->\r\n      <el-table :data=\"carts\" stripe style=\"width: 100%\" :show-header=\"false\">\r\n        <el-table-column label=\"商品图片\" width=\"150\">\r\n          <template slot-scope=\"scope\">\r\n            <el-image :src=\"scope.row.goods.imgs[0]\" style=\"width: 100px; height: 100px;\" fit=\"contain\"></el-image>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"goods.name\" label=\"商品名称\"></el-table-column>\r\n        <el-table-column label=\"价格\">\r\n          <template slot-scope=\"scope\">\r\n            <span v-html=\"scope.row.goods.realPrice + ' X ' + scope.row.count \"></span>\r\n          </template>\r\n        </el-table-column>\r\n\r\n      </el-table>\r\n\r\n      <div style=\"margin-top: 10px\">\r\n        <div style=\"background-color: white; padding: 10px\">\r\n          <div style=\"color: red; text-align: right\">\r\n            <div>\r\n              <span>总价：</span>\r\n              <span>￥ {{ totalPrice }}</span>\r\n            </div>\r\n            <div style=\"text-align: right; color: #999; font-size: 12px; padding: 5px 0\">\r\n              运费： ￥ 0\r\n            </div>\r\n            <div style=\"text-align: right; color: #999; font-size: 12px;\">\r\n              优惠： - ￥{{ discount }}\r\n            </div>\r\n            <div style=\"padding: 10px 0\">\r\n              <el-button style=\"background-color: red; color: white; width: 100px\" @click=\"submitOrder\">提交订单</el-button>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport API from \"@/utils/request\";\r\n\r\nexport default {\r\n  name: \"cart\",\r\n  data() {\r\n    return {\r\n      chooseAddress: null,\r\n      user: {},\r\n      addressData: [],\r\n      carts: [],\r\n      totalPrice: 0,\r\n      discount: 0\r\n    }\r\n  },\r\n  created() {\r\n    this.user = sessionStorage.getItem(\"user\") ? JSON.parse(sessionStorage.getItem(\"user\")) : {}\r\n    if (!this.user) {\r\n      this.$message({\r\n        type: \"warning\",\r\n        message: '请登录！'\r\n      })\r\n      return\r\n    }\r\n    this.load()\r\n\r\n  },\r\n  methods: {\r\n    submitOrder() {\r\n      console.log(this.chooseAddress)\r\n      if (!this.chooseAddress) {\r\n        this.$message({\r\n          type: 'warning',\r\n          message: '请选择收货地址！'\r\n        })\r\n        return\r\n      }\r\n\r\n      // 提交订单\r\n      API.post(\"/api/order\", {\r\n        totalPrice: this.totalPrice,\r\n        linkUser: this.chooseAddress.linkUser,\r\n        linkPhone: this.chooseAddress.linkPhone,\r\n        linkAddress: this.chooseAddress.linkAddress,\r\n        carts: JSON.stringify(this.carts),\r\n        type: this.$store.state.type\r\n      }).then(res => {\r\n        if (res.code === '0') {\r\n          this.$message({\r\n            type: 'success',\r\n            message: '提交成功！'\r\n          })\r\n          this.$router.replace(\"/front/order\")\r\n        } else {\r\n          this.$message({\r\n            type: 'error',\r\n            message: res.msg\r\n          })\r\n        }\r\n      })\r\n    },\r\n    load() {\r\n      this.carts = this.$store.state.carts\r\n\r\n      API.post(\"/api/order/pre\", {carts: JSON.stringify(this.carts)}).then(res => {\r\n        this.carts = res.data.list\r\n        this.totalPrice = res.data.totalPrice\r\n        this.discount = res.data.discount\r\n\r\n        // 处理图片\r\n        this.carts.forEach(item => {\r\n          console.log(item)\r\n          if (!item.goods.imgs) {\r\n            item.goods.imgs = ['']\r\n          } else {\r\n            item.goods.imgs = JSON.parse(item.goods.imgs)\r\n          }\r\n        })\r\n\r\n      })\r\n\r\n      // 获取收货地址\r\n      API.get(\"/api/address\").then(res => {\r\n        this.addressData = res.data\r\n      })\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>\r\n"],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA0DA,OAAAA,GAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,aAAA;MACAC,IAAA;MACAC,WAAA;MACAC,KAAA;MACAC,UAAA;MACAC,QAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAL,IAAA,GAAAM,cAAA,CAAAC,OAAA,WAAAC,IAAA,CAAAC,KAAA,CAAAH,cAAA,CAAAC,OAAA;IACA,UAAAP,IAAA;MACA,KAAAU,QAAA;QACAC,IAAA;QACAC,OAAA;MACA;MACA;IACA;IACA,KAAAC,IAAA;EAEA;EACAC,OAAA;IACAC,YAAA;MACAC,OAAA,CAAAC,GAAA,MAAAlB,aAAA;MACA,UAAAA,aAAA;QACA,KAAAW,QAAA;UACAC,IAAA;UACAC,OAAA;QACA;QACA;MACA;;MAEA;MACAhB,GAAA,CAAAsB,IAAA;QACAf,UAAA,OAAAA,UAAA;QACAgB,QAAA,OAAApB,aAAA,CAAAoB,QAAA;QACAC,SAAA,OAAArB,aAAA,CAAAqB,SAAA;QACAC,WAAA,OAAAtB,aAAA,CAAAsB,WAAA;QACAnB,KAAA,EAAAM,IAAA,CAAAc,SAAA,MAAApB,KAAA;QACAS,IAAA,OAAAY,MAAA,CAAAC,KAAA,CAAAb;MACA,GAAAc,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAjB,QAAA;YACAC,IAAA;YACAC,OAAA;UACA;UACA,KAAAgB,OAAA,CAAAC,OAAA;QACA;UACA,KAAAnB,QAAA;YACAC,IAAA;YACAC,OAAA,EAAAc,GAAA,CAAAI;UACA;QACA;MACA;IACA;IACAjB,KAAA;MACA,KAAAX,KAAA,QAAAqB,MAAA,CAAAC,KAAA,CAAAtB,KAAA;MAEAN,GAAA,CAAAsB,IAAA;QAAAhB,KAAA,EAAAM,IAAA,CAAAc,SAAA,MAAApB,KAAA;MAAA,GAAAuB,IAAA,CAAAC,GAAA;QACA,KAAAxB,KAAA,GAAAwB,GAAA,CAAA5B,IAAA,CAAAiC,IAAA;QACA,KAAA5B,UAAA,GAAAuB,GAAA,CAAA5B,IAAA,CAAAK,UAAA;QACA,KAAAC,QAAA,GAAAsB,GAAA,CAAA5B,IAAA,CAAAM,QAAA;;QAEA;QACA,KAAAF,KAAA,CAAA8B,OAAA,CAAAC,IAAA;UACAjB,OAAA,CAAAC,GAAA,CAAAgB,IAAA;UACA,KAAAA,IAAA,CAAAC,KAAA,CAAAC,IAAA;YACAF,IAAA,CAAAC,KAAA,CAAAC,IAAA;UACA;YACAF,IAAA,CAAAC,KAAA,CAAAC,IAAA,GAAA3B,IAAA,CAAAC,KAAA,CAAAwB,IAAA,CAAAC,KAAA,CAAAC,IAAA;UACA;QACA;MAEA;;MAEA;MACAvC,GAAA,CAAAwC,GAAA,